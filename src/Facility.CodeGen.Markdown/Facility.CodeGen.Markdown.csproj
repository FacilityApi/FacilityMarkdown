<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Used by fsdgenmd to generate Markdown for a Facility Service Definition.</Description>
    <PackageTags>Facility FSD Markdown CodeGen</PackageTags>
    <IsPackable>true</IsPackable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Facility.Definition" Version="2.2.1" />
    <PackageReference Include="Scriban" Version="2.1.3" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="template.scriban-txt" />
  </ItemGroup>

</Project>
